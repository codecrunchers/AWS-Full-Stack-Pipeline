[
    {
        "essential": true,
        "image": "${image_url}",
        "memoryReservation": ${memory},
        "name": "${container_name}",
        "dnsServers": ["${consul_ip}"],
        "environment": [
            {
                "name":"JENKINS_URL",
                "value":"http://jenkins-8080.service.${region}.consul:8080/jenkins"
            },
            {
                "name":"JENKINS_SECRET",
                "value":"4620290824a1c3e4055bff953597d47a05f9e4f8212a44874ce581d5fd046be5"
            },
            {
                "name":"JENKINS_AGENT_NAME",
                "value":"ecs_slave"
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${log_group_name}",
                "awslogs-region": "${log_group_region}"
            }
        }     

    }
]



